{
  "name": "publish-browser-extension",
  "description": "Easily publish chrome/browser extensions to their stores",
  "version": "1.1.4",
  "license": "MIT",
  "keywords": [
    "chrome-web-store",
    "firefox-addon-store",
    "chrome",
    "firefox",
    "addon",
    "store",
    "extension",
    "browser"
  ],
  "author": {
    "name": "Aaron Klinker",
    "url": "https://github.com/aklinker1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aklinker1/publish-browser-extension"
  },
  "homepage": "https://github.com/aklinker1/publish-browser-extension#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "publish-extension": "dist/cli.cjs"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "compile": "tsc --noEmit",
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:lib": "tsup src/index.ts --dts --format cjs,esm",
    "build:cli": "tsup src/cli.ts",
    "build:test-extension": "node scripts/build-test-extension.js",
    "test": "echo 'TODO'",
    "test:watch": "echo 'TODO'",
    "dev": "env-cmd ./scripts/dev.sh",
    "dev:all": "env-cmd ./scripts/dev.sh all",
    "dev:chrome": "env-cmd ./scripts/dev.sh chrome",
    "dev:firefox": "env-cmd ./scripts/dev.sh firefox",
    "dev:help": "env-cmd ./scripts/dev.sh help",
    "generate-firefox-token": "env-cmd esno scripts/firefox-token.ts",
    "prepack": "pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "cli-highlight": "^2.1.11",
    "extract-zip": "^2.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "ofetch": "^1.0.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/node": "^18",
    "@types/node-fetch": "^2",
    "archiver": "^5.3.1",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.14.42",
    "esbuild-node-externals": "^1.4.1",
    "form-data": "^4.0.0",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.4.0",
    "rollup": "^2.75.3",
    "run-all": "^1.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.7.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/download",
        "download",
        "express",
        "safe-compare",
        "body-parser"
      ]
    }
  }
}
