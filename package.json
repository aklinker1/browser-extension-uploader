{
  "name": "publish-browser-extension",
  "description": "Easily publish web extensions to their stores",
  "version": "1.4.1",
  "engines": {
    "node": "18",
    "pnpm": "8"
  },
  "license": "MIT",
  "keywords": [
    "chrome-web-store",
    "firefox-addon-store",
    "chrome",
    "firefox",
    "addon",
    "store",
    "extension",
    "browser"
  ],
  "author": {
    "name": "Aaron Klinker",
    "url": "https://github.com/aklinker1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aklinker1/publish-browser-extension"
  },
  "homepage": "https://github.com/aklinker1/publish-browser-extension#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "publish-extension": "dist/cli.cjs",
    "chrome-refresh-token": "dist/chrome-refresh-token.cjs"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "compile": "tsc --noEmit",
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:lib": "tsup src/index.ts --dts --format cjs,esm --sourcemap",
    "build:cli": "tsup --entry.cli src/cli/index.ts --sourcemap",
    "build:test-extension": "node scripts/build-test-extension.js",
    "test": "echo 'TODO'",
    "test:watch": "echo 'TODO'",
    "dev:all": "./scripts/dev.sh all",
    "dev:chrome": "./scripts/dev.sh chrome",
    "dev:firefox": "./scripts/dev.sh firefox",
    "dev:edge": "./scripts/dev.sh edge",
    "dev:help": "./scripts/dev.sh help",
    "publish-extension": "tsx src/cli/index.ts",
    "prepack": "pnpm build",
    "prepare": "husky install"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "cli-highlight": "^2.1.11",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "extract-zip": "^2.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.snakecase": "^4.1.1",
    "node-fetch": "^2",
    "open": "^9.1.0",
    "ora": "^6.3.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/lodash.snakecase": "^4.1.7",
    "@types/node": "^18",
    "@types/node-fetch": "^2",
    "@types/prompts": "^2.4.4",
    "archiver": "^5.3.1",
    "form-data": "^4.0.0",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.4.0",
    "run-all": "^1.0.1",
    "tsup": "^6.6.3",
    "tsx": "^3.12.5",
    "typescript": "^4.7.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/download",
        "download",
        "express",
        "safe-compare",
        "body-parser"
      ]
    }
  }
}
